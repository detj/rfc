{
  "name": "rfc",
  "version": "0.0.11",
  "description": "IETF RFC reader tool",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "rfc": "./bin/rfc"
  },
  "man": "./rfc.1",

    "rmrf": "~1.0.2",
    "superagent": "^1.7.1",
    "through": "~2.3.8"
  },
  "devDependencies": {
    "jsdoc": "^3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jwerle/rfc.git"
  },
